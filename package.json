  "bugs": {
    "url": "https://github.com/css-modules/postcss-modules-extract-imports/issues"
  },
  "homepage": "https://github.com/css-modules/postcss-modules-extract-imports",
  "devDependencies": {
    "codecov.io": "^0.1.6",
    "coveralls": "^3.1.0",
    "eslint": "^7.9.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "postcss": "^8.0.6",
    "prettier": "^2.1.2"
  },
  "peerDependencies": {
    "postcss": "^8.0.6"
  }
