{
  "name": "postcss-modules-extract-imports",
  "version": "3.0.0-rc.0",
  "description": "A CSS Modules transform to extract local aliases for inline imports",
  "main": "src/index.js",
  "engines": {
    "node": ">= 10.13.0 || >= 12.13.0 || >= 14"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "prettier": "prettier -l --ignore-path .gitignore . \"!test/test-cases\"",
    "eslint": "eslint --ignore-path .gitignore .",
    "lint": "yarn eslint && yarn prettier",
    "pretest": "yarn lint",
    "test": "jest --coverage",
    "autotest": "jest --coverage --watch",
    "cover": "jest --coverage",
    "ci": "yarn pretest && yarn cover",
    "prepublishOnly": "yarn test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/css-modules/postcss-modules-extract-imports.git"
  },
